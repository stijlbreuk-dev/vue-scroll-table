{
    "name": "vue-scroll-table",
    "version": "0.1.1",
    "description": "A Vue table component with horizontal scrolling and a sticky first column.",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
        "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-scroll-table.umd.js",
        "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-scroll-table.esm.js",
        "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-scroll-table.min.js --environment SB_MINIFY"
    },
    "main": "dist/vue-scroll-table.umd.js",
    "module": "dist/vue-scroll-table.esm.js",
    "unpkg": "dist/vue-scroll-table.min.js",
    "browser": {
        "./sfc": "src/ScrollTable.vue"
    },
    "author": "Jeroen Peerbolte <jeroen@stijlbreuk.nl>",
    "repository": {
        "type": "git",
        "url": "https://github.com/stijlbreuk/vue-scroll-table.git"
    },
    "license": "MIT",
    "devDependencies": {
        "@vue/cli-plugin-eslint": "^3.1.3",
        "node-sass": "^4.9.4",
        "rollup": "^0.67.0",
        "rollup-plugin-buble": "^0.19.4",
        "rollup-plugin-node-resolve": "^3.4.0",
        "rollup-plugin-uglify": "^6.0.0",
        "rollup-plugin-vue": "^4.3.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.5.17"
    },
    "dependencies": {
        "deepmerge": "2.2.1"
    },
    "keywords": [
        "vue",
        "vuejs",
        "vue.js",
        "table",
        "scroll",
        "sticky",
        "column"
    ]
}
